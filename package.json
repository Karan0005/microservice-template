{
  "name": "corporate",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start": "nx serve",
    "build": "nx build",
    "test": "nx test",
    "infrastructure:build": "docker-compose -f infrastructure/local/docker-compose.yml up --build --remove-orphans",
    "infrastructure:down": "docker-compose -f infrastructure/local/docker-compose.yml down --remove-orphans",
    "backend-base:serve": "dapr run --components-path ./infrastructure/local/dapr/components --dapr-http-port 3500 --app-id 'backend-base' --app-port 8000 nx serve backend-base",
    "backend-base:precommit": "eslint --fix ./apps/backend/base && prettier --config .prettierrc ./apps/backend/base --write && nx test backend-base --skip-nx-cache && nx build backend-base --prod --progress --optimization --skip-nx-cache --generatePackageJson=true",
    "backend-notification:serve": "dapr run --components-path ./infrastructure/local/dapr/components --dapr-http-port 3600 --app-id 'backend-notification' --app-port 8100 nx serve backend-notification",
    "backend-notification:precommit": "eslint --fix ./apps/backend/notification && prettier --config .prettierrc ./apps/backend/notification --write && nx test backend-notification --skip-nx-cache && nx build backend-notification --prod --progress --optimization --skip-nx-cache --generatePackageJson=true"
  },
  "private": true,
  "devDependencies": {
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.0.0",
    "@nrwl/cli": "13.3.12",
    "@nrwl/eslint-plugin-nx": "13.3.12",
    "@nrwl/jest": "13.3.12",
    "@nrwl/linter": "13.3.12",
    "@nrwl/nest": "^13.3.12",
    "@nrwl/node": "13.3.12",
    "@nrwl/tao": "13.3.12",
    "@nrwl/workspace": "13.3.12",
    "@types/express": "^4.17.13",
    "@types/jest": "27.0.2",
    "@types/node": "^14.14.33",
    "@types/pubnub": "^4.29.5",
    "@types/uuid": "^8.3.3",
    "@typescript-eslint/eslint-plugin": "~5.3.0",
    "@typescript-eslint/parser": "~5.3.0",
    "eslint": "8.2.0",
    "eslint-config-prettier": "8.1.0",
    "jest": "27.2.3",
    "prettier": "^2.3.1",
    "ts-jest": "27.0.5",
    "typescript": "~4.4.3"
  },
  "dependencies": {
    "@nestjs/axios": "^0.0.3",
    "@nestjs/common": "^8.0.0",
    "@nestjs/config": "^1.1.5",
    "@nestjs/core": "^8.0.0",
    "@nestjs/graphql": "^9.1.2",
    "@nestjs/platform-express": "^8.0.0",
    "@nestjs/swagger": "^5.1.5",
    "@nestjs/terminus": "^8.0.3",
    "@nestjs/typeorm": "^8.0.2",
    "@sendgrid/mail": "^7.6.0",
    "@sentry/node": "^6.16.1",
    "apollo-server": "^3.5.0",
    "apollo-server-core": "^3.5.0",
    "apollo-server-express": "^3.5.0",
    "apollo-server-types": "^3.4.0",
    "class-validator": "^0.13.2",
    "express": "^4.17.2",
    "graphql": "^15.8.0",
    "logform": "^2.3.0",
    "mongodb": "^3.7.3",
    "mssql": "^7.3.0",
    "nest-winston": "^1.6.2",
    "pubnub": "^4.37.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.0.0",
    "swagger-ui-express": "^4.3.0",
    "tslib": "^2.0.0",
    "typeorm": "^0.2.41",
    "uuid": "^8.3.2",
    "winston": "^3.3.3",
    "winston-transport-sentry-node": "^2.0.0"
  }
}
